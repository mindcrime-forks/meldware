<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mbean PUBLIC
   "-//JBoss//DTD JBOSS XMBEAN 1.2//EN"
   "http://www.jboss.org/j2ee/dtd/jboss_xmbean_1_0.dtd">


<mbean>
    <description></description>
    <class>org.buni.meldware.mail.mailhandler.localmailbox.MimeParserListener</class>
    <constructor>
        <description>The no-arg constructor</description>
        <name>org.buni.meldware.mail.mailhandler.localmailbox.MimeParserListener</name>
    </constructor>

    <attribute access="write-only" setMethod="setMailBodyManager">
        <name>MailBodyManager</name>
        <type>org.buni.meldware.mail.message.MailBodyManager</type>
    </attribute>

    <operation>
        <name>send</name>
        <parameter>
            <name>msg</name>
            <type>org.buni.meldware.mail.message.Message</type>
        </parameter>
        <return-type>org.buni.meldware.mail.message.Message</return-type>
    </operation>

</mbean>
